<?xml version="1.0" encoding="utf-8" ?>
<openerp>
	<data>
		<template
			id="snippet_product_category_carousel_73lines.new_category_item"
			name="Category item">
			<div class="owl-item-custom">
				<div class="oe_category_image ">
					<a t-att-href="'/shop/category/%s' % slug(category)">
						<span itemprop="image" t-field="category.image"
							t-field-options='{"widget": "image" #{None if image else ", \"resize\": \"300x300\""}}'
							t-att-alt="category.name" class="" />
					</a>
				</div>
			</div>
		</template>
	</data>
</openerp>